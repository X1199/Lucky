print("=" * 5, "", "=" * 5)
X = [1, 2, 3, 4, 5,]
print(" : ", X)

X.append(6)
print("batu bata yang ditambahkan menjadi    : ", 6)
print("batu bata yang diangkut : ", X)

X.append(7)
print(" batu bata yang ditambahkan menjadi   : ", 7)
print("batu bata yang diangkut: ", X)

X.pop() 
print("batu bata yang dikeluarkan: ",7)


print("jumlah bangku yang diangkut : ", X)

print('\n\n')
